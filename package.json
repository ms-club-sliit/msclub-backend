		"@types/cors": "^2.8.12",
		"@types/firebase": "^3.2.1",
		"@types/handlebars": "^4.1.0",
		"@types/imagemin": "^7.0.1",
		"@types/jsonwebtoken": "^8.5.6",
		"@types/mongoose": "^5.11.97",
		"@types/multer": "^1.4.7",
		"@types/nodemailer": "^6.4.4",
		"@types/sharp": "^0.29.4",
		"@types/uuid": "^8.3.3",
		"@types/validator": "^13.7.0",
		"@types/whatwg-url": "^8.2.1",
		"amqplib": "^0.8.0",
		"axios": "^0.24.0",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"cross-fetch": "^3.1.4",
		"dayjs": "^1.10.7",
		"dotenv": "^10.0.0",
		"env-cmd": "^10.1.0",
		"express": "^4.17.1",
